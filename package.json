{
  "name": "svelte-lib-helpers",
  "version": "0.3.0",
  "description": "A utility package for Svelte libraries.",
  "type": "module",
  "main": "index.js",
  "bin": {
    "svelte-lib-helpers": "index.js"
  },
  "scripts": {
    "add-exports": "./index.js exports",
    "add-docs": "./index.js docs",
    "add-package": "./index.js package",
    "add-data": "./index.js compo-data",
    "gen:exports": "svelte-lib-helpers exports",
    "gen:docs": "svelte-lib-helpers docs",
    "gen:compo-data": "svelte-lib-helpers compo-data",
    "copy:package": "svelte-lib-helpers package",
    "lib-helpers": "npm run gen:docs && npm run gen:compo-data && npm run build && npm run gen:exports && npm run copy:package",
    "package:publish": "standard-version && git push --follow-tags origin main && npm run lib-helpers && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinokada/svelte-lib-helpers.git"
  },
  "keywords": [
    "svelte",
    "sveltekit",
    "prop",
    "package",
    "exports"
  ],
  "author": "Shinichi Okada",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shinokada/svelte-lib-helpers/issues"
  },
  "homepage": "https://github.com/shinokada/svelte-lib-helpers#readme"
}
